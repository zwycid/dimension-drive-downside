package mju.summer2012.itproject.team3.lk.sailingtext;

import android.content.Context;
import android.util.AttributeSet;
import android.widget.TextView;

public class BlankView extends TextView{

	public BlankView(Context context, AttributeSet attrs, int defStyle) {
		super(context, attrs, defStyle);
	}

	public BlankView(Context context, AttributeSet attrs) {
		super(context, attrs);
	}

	public BlankView(Context context) {
		super(context);
	}

}
